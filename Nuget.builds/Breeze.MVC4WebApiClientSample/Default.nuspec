<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>{{id}}</id>
        <version>{{version}}</version>
        <title>Breeze for MVC4 Web Api Client Sample</title>
        <authors>Jay Traband</authors>
        <owners>IdeaBlade</owners>
        <licenseUrl>http://www.breezejs.com/license</licenseUrl>
        <projectUrl>http://www.breezejs.com</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Adds a very simple Breeze sample app to the project.  We recommend installing this package shortly after creating a project with the "ASP.NET MVC 4 Empty" template or the "ASP.NET MVC 4 Web Api" template. It extends the dependent "Breeze for WebApi projects" package which will be installed automatically.</description>
        <summary>Breeze MVC4 Web Api client sample application</summary>
        <copyright>Copyright IdeaBlade 2012</copyright>
        <language>en-US</language>
        <releaseNotes>Please review the BreezeJS release notes at http://www.breezejs.com/documentation/download</releaseNotes>
        <tags>javascript MVC4 EntityFramework Breeze</tags>
		<dependencies>
			<dependency id="Breeze.WebApi" version="{{version}}" />
            <!-- Force version #s to align with the MVC 4 Web API template -->
            <dependency id="jQuery" version="[1.7.1, 1.7.2)" />
            <dependency id="knockoutjs" version="[2.1.0, 2.1.1)" />
            <dependency id="Modernizr" version="[2.5.3, 2.5.4)" />
		</dependencies>
    </metadata>
    <files>
        <file src="content\App_Start\BreezeClientSampleConfig.cs.pp" target="content\App_Start\BreezeClientSampleConfig.cs.pp" />
        <file src="content\Content\breezesample.css" target="content\Content\breezesample.css" />
        <file src="content\Controllers\BreezeSampleController.cs.pp" target="content\Controllers\BreezeSampleController.cs.pp" />
        <file src="content\Models\BreezeSampleContext.cs.pp" target="content\Models\BreezeSampleContext.cs.pp" />
        <file src="content\Models\BreezeSampleDatabaseInitializer.cs.pp" target="content\Models\BreezeSampleDatabaseInitializer.cs.pp" />
        <file src="content\Models\BreezeSampleTodoItem.cs.pp" target="content\Models\BreezeSampleTodoItem.cs.pp" />
        <file src="content\Controllers\BreezeSampleShellController.cs.pp" target="content\Controllers\BreezeSampleShellController.cs.pp" />
        <file src="content\Views\BreezeSampleShell\index.cshtml" target="content\Views\BreezeSampleShell\index.cshtml" />
        <file src="content\Scripts\app\logger.js" target="content\Scripts\app\logger.js" />
		<file src="content\Scripts\app\sampleViewModel.js" target="content\Scripts\app\sampleViewModel.js" />
        <file src="readme.txt" target="readme.txt" />
    </files>
</package>
